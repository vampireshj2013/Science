<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />

    <package name="default" namespace="/" extends="struts-default">

        <default-action-ref name="index" />

        <global-results>
            <result name="error">/error.jsp</result>
        </global-results>

        <global-exception-mappings>
            <exception-mapping exception="java.lang.Exception" result="error"/>
        </global-exception-mappings>

        <action name="demo" class="com.science.action.Demo">
            <result name="success">/structs.jsp</result>
        </action>
        <action name="sciAction_*" class="com.science.action.SciAchevementAction" method="{1}">
            <result name="updateInit">/update_SciAchievement.jsp</result>
            <result name="list">/sciAchievementList.jsp</result>
            
        </action>
        <!--  <action name="patentTranAction_*" class="com.science.action.PatentTranAction" method="{1}">
            <result name="updateInit">/update_PatentTran.jsp</result> 
          
            <result name="list">/patentTranList.jsp</result>
            <result name="success">/structs.jsp</result>
        </action> -->
         <action name="patentTranAction_*" class="com.science.action.PatentTranAction" method="{1}">
            <result name="success">/structs.jsp</result>
            <result name="updateInit">/update_PatentTran.jsp</result> 
            <result name="list">/patentTranList.jsp</result>
            </action>
    </package>

    <include file="example.xml"/>

    <!-- Add packages here -->

</struts>
